  ldes:pollingInterval '3600'^^xsd:integer ;
  tree:shape [
    a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:property [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path brol:doare
    ],
    [
      sh:datatype xsd:dateTime ;
      sh:minCount 1 ;
      sh:path brol:hiere
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:altLabel
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:prefLabel
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:definition
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:notation
    ];
    sh:targetClass skos:Concept
  ] ;
  tree:view <> ;
  .

